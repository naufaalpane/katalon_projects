<div class="row customContainer mt-4">
    <div class="col-md-7">
        <div class="card customShadow">
            <div class="card-header pb-1">
                <div class="row">
                    <div class="col-md-6">
                        <h3 class="card-title">
                            K/M. Prod Plan Creation
                        </h3>
                    </div>
                    <div class="col-md-6 text-right">
                        <div class="form-group-sm">
                            <h5 class="text-right"><span class="badge companyCd">Company : <span id="comcd">112B</span></span></h5>
                        </div>
                    </div>
                </div>
            </div>
            <div class="card-body">
                <div class="row mb-3">
                    <div class="col-md-12">
                        <div class="form-inline">
                            <div class="form-group">
                                <label for="statuscd" class="font-weight-bolder ml-2">PROCESS :</label>
                                <select class="form-control ml-2" id="proccd" onchange="changeProcessCd()" style="width: 85px;">
                                    <option value=""> Choose..</option>
                                    <option value="K">K</option>
                                    <option value="M">M</option>
                                </select>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row mt-1 justify-content-center">
                    <div class="col-md-8">
                        <div id="gridTableCourse">
                            @Html.Partial("_GridView")
                        </div>
                    </div>
                </div>
                <div class="row mt-2">
                    <div class="col-md-9">
                        <div class="form-inline">
                            <div class="input-group" id="MonthDatePicker">
                                <label class="font-weight-bolder ml-2">Date : </label>
                                <input class="form-control month-picker ml-2 col-md-5" id="txtDate" type="text" data-date-format="dd.mm.yyyy" data-name="txtDate" placeholder="DDMMYYYY" />
                                <div class="input-group-append">
                                    <span class="input-group-text">
                                        <i class="far fa-calendar-alt"></i>
                                    </span>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="form-group-sm text-right">
                            <button type="button" id="btn-search" onclick="javascript: doExecute()" class="btn btn-info">
                                <i class="fa fa-check-circle"></i> Execute
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>