<div class="row customContainer mt-4">
    <div class="col-md-12">
        <div class="card customShadow">
            <div class="card-header pb-1">
                <div class="row">
                    <div class="col-md-6">
                        <h3 class="card-title">
                            Data Extraction
                        </h3>
                    </div>
                    <div class="col-md-6 text-right">
                        <div class="form-group-sm">
                            <h5 class="text-right"><span class="badge companyCd">Company : <span id="comcd">112B</span></span></h5>
                        </div>
                    </div>
                </div>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="col-md-10">
                        <div class="form-inline">
                            <div class="form-group">
                                <label for="processcd" class="font-weight-bolder ml-2">Data Extraction Start Month</label>
                            </div>
                            <div class="form-group ml-6" style="width: 100px;">
                                <label for="year" class="font-weight-bolder ml-2"><span class="mandatory">*</span>Year:</label>
                                <input type="number" name="year" class="form-control ml-2 col-md-6" placeholder="YYYY" maxlength="4" />
                            </div>
                            <div class="form-group" style="width: 110px;">
                                <label for="month" class="font-weight-bolder ml-2"><span class="mandatory">*</span>Month:</label>
                                <input type="number" name="month" class="form-control ml-2 col-md-4" placeholder="MM" maxlength="2"/>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row mt-2 mb-2">
                    <div class="col-md-7">
                        <div id="gridTableCourse">
                            <fieldset>
                                <legend style="margin-bottom: 0px;"><small style="display: block; text-align: center;"><label style="margin: 0px;">Data</label></small></legend>
                                <div class="form-check label-a pt-0 pb-2">
                                    <input class="form-check-input" type="radio" name="radiosC" id="radiosDeliveryPackingPlan" value="option1">
                                    <label class="form-check-label" for="radiosDeliveryPackingPlan">
                                        Delivery Packing Plan
                                    </label>
                                </div>
                                <div class="form-check label-a2 pt-0 pb-2">
                                    <input class="form-check-input" type="radio" name="radiosC" id="radiosProductionPlan" value="option2">
                                    <label class="form-check-label" for="radiosProductionPlan">
                                        Production Plan
                                    </label>
                                </div>
                                <div class="form-check label-a2 pt-0 pb-2">
                                    <input class="form-check-input" type="radio" name="radiosC" id="radiosTheoryStock" value="option2">
                                    <label class="form-check-label" for="radiosTheoryStock">
                                        Theory Stock
                                    </label>
                                </div>
                                <div class="form-check label-a2 pt-0 pb-2">
                                    <input class="form-check-input" type="radio" name="radiosC" id="radiosNQCResult" value="option2">
                                    <label class="form-check-label" for="radiosNQCResult">
                                        NQC Result (Batch)
                                    </label>
                                </div>
                            </fieldset>
                        </div>
                    </div>
                    <div class="col-md-5 text-right d-flex align-items-end">
                        <div class="col-md-12 text-right">
                            <button type="button" id="btn-download" class="btn btn-primary" onclick="onDownload()">Download</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>