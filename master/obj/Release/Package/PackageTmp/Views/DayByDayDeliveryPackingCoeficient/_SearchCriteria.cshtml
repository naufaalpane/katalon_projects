<div class="row customContainer mt-4">
    <div class="col-md-12">
        <div class="card customShadow">
            <div class="card-header pb-1">
                <div class="row">
                    <div class="col-md-9 col-9 col-sm-9">
                        <h3 class="card-title">
                            Day by Day Delivery/Packing Coefficient Screen
                        </h3>
                    </div>

                    <div class="col-md-3 col-3 col-sm-3">
                        <div class="form-group-sm">
                            <h5 class="text-right"><span class="badge companyCd">Company : <span id="comcd">112B</span></span></h5>
                        </div>
                    </div>
                </div>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="col-md-8">
                        <div class="form-inline">
                            <div class="form-group ml-2">
                                <div class="form-group ml-2">
                                    <div class="form-group row">
                                        <label class="col-5 col-sm-5 col-md-5">Year Month : </label>

                                        <div class="input-group col-7 col-sm-7 col-md-7">
                                            <input class="form-control month-picker" id="txtYear" type="text" data-date-format="dd.mm.yyyy" data-name="txtYear"/>
                                            <div class="input-group-append">
                                                <span class="input-group-text">
                                                    <i class="far fa-calendar-alt"></i>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4 text-right">
                        <div class="form-group-sm">
                            <div class="text-right">
                                <button class="btn btn-md btn-default initialBtn" onclick="btnClear_onClick()">Clear</button>
                                <button class="btn btn-md btn-info initialBtn" onclick="btnSearch_onClick()">Search</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>