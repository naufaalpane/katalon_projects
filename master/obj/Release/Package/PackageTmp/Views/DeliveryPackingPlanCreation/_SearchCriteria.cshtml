<div class="row customContainer mt-4">
    <div class="col-md-8">
        <div class="card customShadow">
            <div class="card-header">
                <h3 class="card-title">
                    Display order excepting self-unit plant
                </h3>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-inline">
                            <div class="form-group">
                                <label for="processcd" class="font-weight-bolder ml-2">PROCESS :</label>
                                <input type="text" name="processcd" class="form-control col-md-3 ml-2" />
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group-sm">
                            <h5 class="text-right"><span class="badge companyCd">Company : <span id="comcd">112B</span></span></h5>
                        </div>
                    </div>
                </div>
                <div class="row mt-2">
                    <div class="col-md-7">
                        <div id="gridTableCourse">
                            @Html.Partial("_GridView")
                        </div>
                    </div>
                    <div class="col-md-5">
                        <div class="form-group-sm text-center" style="border: 1px solid #BFC0C1; border-radius: 10px;">
                            <div class="row">
                                <div class="col-md-12 text-center">
                                    <strong class="text-center" style="color: #8A8A8A;">NQC Executed date</strong>
                                </div>
                            </div>
                            <div class="row mt-2">
                                <div class="col-md-12 text-center">
                                    <h5 style="color: #8A8A8A;"><u>YYYY/MM/DD HH:MM:SS</u></h5>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row mt-2">
                    <div class="col-md-10">
                        <div class="form-inline">
                            <div class="form-group">
                                <label for="cfc" class="font-weight-bolder mr-2">Delivery/Packing Plan Creation Start Date :</label>
                            </div>
                            <div class="form-group" style="width: 100px;">
                                <label for="year" class="font-weight-bolder ml-2">Year :</label>
                                <input type="text" name="year" class="form-control ml-2 col-md-6" placeholder="YYYY" />
                            </div>
                            <div class="form-group" style="width: 100px;">
                                <label for="month" class="font-weight-bolder ml-2">Month :</label>
                                <input type="text" name="month" class="form-control ml-2 col-md-4" placeholder="MM" />
                            </div>
                            <div class="form-group" style="width: 80px;">
                                <label for="day" class="font-weight-bolder ml-2">Day :</label>
                                <input type="text" name="day" class="form-control ml-2 col-md-5" placeholder="YY" />
                            </div>
                        </div>
                    </div>
                    <div class="col-md-2">
                        <div class="form-group-sm text-right">
                            <button type="button" id="btn-search" class="btn btn-info">
                                <i class="fa fa-check-circle"></i> Execute
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>