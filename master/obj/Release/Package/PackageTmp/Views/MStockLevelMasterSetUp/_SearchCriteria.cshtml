<div class="row customContainer mt-4">
    <div class="col-md-12">
        <div class="card customShadow">
            <div class="card-header pb-1">
                <div class="row">
                    <div class="col-md-9">
                        <h3 class="card-title">
                            <i class="fas fa-search"></i>
                            Search Criteria
                        </h3>
                    </div>

                    <div class="col-md-3">
                        <div class="form-group-sm">
                            <h5 class="text-right"><span class="badge companyCd">Company : <span id="comcd">112B</span></span></h5>
                        </div>
                    </div>
                </div>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-inline">
                            <div class="form-group">
                                <label for="proccd" class="font-weight-bolder ml-2">PROCESS :</label>
                                <select class="form-control ml-2" id="proccd" style="width: 65px;">
                                    <option value=""> Choose</option>
                                    <option value="k">K</option>
                                    <option value="m">M</option>
                                </select>
                            </div>
                            <div class="form-group ml-3">
                                <label for="linecd" class="font-weight-bolder">Line Code :</label>
                                <input type="text" id="linecd" name="" class="form-control ml-2" maxlength="4" style="width: 50px" />
                            </div>
                            <div class="form-group ml-3">
                                <label for="cfc" class="font-weight-bolder">Car Family Code :</label>
                                <input type="text" id="cfc" name="" class="form-control ml-2" maxlength="4" style="width: 50px;"/>
                            </div>
                            <div class="form-group ml-3">
                                <label for="partsno" class="font-weight-bolder">Parts No. :</label>
                                <input type="text" name="partsno" id="partsno" class="form-control ml-1" maxlength="5" style="width: 60px" />
                                <strong class="ml-1"><b>-</b></strong>
                                <input type="text" name="partsno" class="form-control ml-1" maxlength="5" style="width: 60px" />
                                <strong class="ml-1"><b>-</b></strong>
                                <input type="text" name="partsno" class="form-control ml-1" maxlength="2" style="width: 30px" />
                            </div>
                            <div class="form-group ml-3">
                                <label for="statuscd" class="font-weight-bolder ml-2">&nbsp;Status Code :</label>
                                <select class="form-control ml-2" style="width: 170px;" id="statuscd">
                                    <option selected>--All--</option>
                                    <option value="0">0 - Trial</option>
                                    <option value="1">1 - Volume Production</option>
                                    <option value="2">2 - CPO</option>
                                    <option value="3">3 - Service Parts (Volume Production)</option>
                                    <option value="4">4 - Service Parts (Past)</option>
                                </select>
                            </div>
                        </div>
                    </div>
                </div><hr />
                <div class="row">
                    <div class="col-12 col-sm-12 col-md-12 text-right">
                        <div class="form-group-sm">
                            <button class="btn btn-md btn-default initialBtn" id="btn_clear">Clear</button>
                            <button class="btn btn-md btn-info initialBtn" id="btn_search" onclick="onSearch()">Search</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>