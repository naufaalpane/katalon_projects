@using GPPSU.Models.Common;
@{
    PagingModel page = ViewData["Paging"] != null ? (PagingModel)ViewData["Paging"] : new PagingModel(0, 1, 10);
}

<script type="text/javascript">
    $(document).ready(function () {
        $("#checkall").click(function () {
            $(".grid-checkbox").prop("checked", $("#checkall").is(":checked"));
        });

        $(".grid-checkbox").click(function () {
            $("#checkall").prop("checked", $('.grid-checkbox:not(#checkall)').not(':checked').length == 0);
        });
    });
</script>

<div class="customContainer" style="margin-top: -10px;">
    <div class="col-md-12">
        <div class="card customShadow">
            <div class="preload">
                <div class="spinnerDisplay"></div>
            </div>
            <div class="card-body sk-loading">
                <div class="row mb-3">
                    <div class="col-md-12 text-right">

                    </div>
                </div>
                <div class="row mb-2">
                    <div class="col-md-12">
                        <div class="table-responsive customTableBorder">
                            <div class="tableScroll">
                                <table class="customTable text-center" id="tblDetail" width="100%">
                                    <thead>
                                        <tr>

                                            <th rowspan="2" class="stickyColumn">Status Code</th>
                                            <th rowspan="2" class="stickyColumn">Destination</th>
                                            <th rowspan="2" class="stickyColumn">Dest Code</th>
                                            <th rowspan="2" class="stickyColumn">ODT</th>
                                            <th rowspan="2" class="stickyColumn">Version</th>
                                            <th rowspan="2" class="stickyColumn">Car Family Code</th>
                                            <th rowspan="2" class="stickyColumn">Prev Sys. Code</th>
                                            <th rowspan="2" class="stickyColumn">Del/Pack Month</th>
                                            <th colspan="2">Recieve</th>
                                            <th colspan="6">Status</th>
                                            <th colspan="3">Last Update</th>
                                        </tr>
                                        <tr>
                                            <th>Date</th>
                                            <th>Time</th>
                                            <th>Order Recieve</th>
                                            <th>Order Check</th>
                                            <th>Order Difference</th>
                                            <th>Day By Day Explotion</th>
                                            <th>K Del/Pack Plan</th>
                                            <th>M Del/Pack Plan</th>
                                            <th>User Name</th>
                                            <th>Date</th>
                                            <th>Time</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @for (int i = 1; i <= 100; i++)
                                        {
                                        <tr>

                                            <td class="stickyColumn"> @i</td>
                                            <td class="stickyColumn">TDV @i</td>
                                            <td class="stickyColumn">807D @i</td>
                                            <td class="stickyColumn">R @i</td>
                                            <td class="stickyColumn">T @i</td>
                                            <td class="stickyColumn">507W @i</td>
                                            <td class="stickyColumn">3 @i</td>
                                            <td class="stickyColumn">2021/02 @i</td>
                                            <td>2021/02/01 @i</td>
                                            <td>16:30 @i</td>
                                            <td>OK @i</td>
                                            <td>OK @i</td>
                                            <td>OK @i</td>
                                            <td>OK @i</td>
                                            <td>OK @i</td>
                                            <td>OK @i</td>
                                            <td>User Name @i</td>
                                            <td>2021/02/01 @i</td>
                                            <td>16:30 @i</td>
                                        </tr>
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row text-right">
                    <div class="col-md-12">
                        <div class="form-group-sm">
                            @Html.Partial("_UIPaging", page)
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>